    {% if securityContext.login %}
    <securitycontext>
      <select id="project">
        <option>select a project ...</option>
        {% for project in securityContext.projects %}
          <option value="{{project}}" {% if currentProject==project %}selected="selected"{% endif %}>{{project}}</option>
        {% endfor %}
      </select>
      {{securityContext.login}} <a href="/logout">logout</a><br />
    </securitycontext>
    {% endif %}
<br />
    <div class="container">{% block body %}{% endblock %}</div>
